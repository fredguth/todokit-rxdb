	"type": "object",
	"indexes": ["createdAt", "updatedAt"],
	"primaryKey": "id",
	"properties": {
		"id": {
			"type": "string",
			"maxLength": 100
		},
		"text": {
			"type": "string",
			"maxLength": 100
		},
		"done": {
			"type": "boolean"
		},
		"createdAt": {
			"type": "number",
			"multipleOf": 1,
			"minimum": 0,
			"maximum": 9000000000000
		},
		"updatedAt": {
			"type": "number",
			"multipleOf": 1,
			"minimum": 0,
			"maximum": 9000000000000
		}
	},
	"required": ["id", "text", "done", "createdAt", "updatedAt"]
}
